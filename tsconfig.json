{
  "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "es2015"],
      "allowJs": true,
      "skipLibCheck": true,
      "outDir": "build",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "es2015",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true,
      "noImplicitAny": false,
      "jsx": "react-jsx"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": ["node_modules", "build/**/*"]
}